{
  "name": "clogy",
  "version": "1.0.2",
  "description": "A logging library for browsers and nodejs environment",
  "homepage": "https://github.com/pgmanutd/clogy",
  "author": {
    "name": "pgmanutd",
    "email": "prashantmanunited@gmail.com",
    "url": "https://github.com/pgmanutd"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/pgmanutd/clogy.git"
  },
  "bugs": {
    "url": "https://github.com/pgmanutd/clogy/issues"
  },
  "main": "lib/clogy.js",
  "engines": {
    "node": ">= 0.6.0"
  },
  "files": [
    "LICENSE",
    "README.md",
    "CHANGELOG.md",
    "CONTRIBUTING.md",
    "lib/"
  ],
  "config": {
    "dirs": {
      "src": "src",
      "dest": "dist",
      "lib": "lib"
    }
  },
  "scripts": {
    "start": "gulp",
    "test": "gulp test",
    "eslint": "gulp eslint",
    "coveralls": "cat ./coverage*/*/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "build": "gulp scripts --unwatch && gulp scripts --type=production && gulp copy",
    "patch": "gulp version:patch && npm run build && gulp year:version && npm publish",
    "minor": "gulp version:minor && npm run build && gulp year:version && npm publish",
    "major": "gulp version:major && npm run build && gulp year:version && npm publish"
  },
  "keywords": [
    "clogy",
    "log",
    "logger",
    "logging",
    "browser"
  ],
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/pgmanutd/clogy/blob/master/LICENSE"
    }
  ],
  "dependencies": {
    "babel-core": "^6.7.6",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-es2015-loose": "^7.0.0",
    "del": "^2.2.0",
    "gulp": "^3.9.1",
    "gulp-help": "^1.6.1",
    "gulp-load-plugins": "^1.2.0",
    "gulp-plumber": "^1.1.0",
    "gulp-replace": "^0.5.4",
    "gulp-util": "^3.0.7",
    "lodash": "^4.13.1",
    "path": "^0.12.7",
    "remove-empty-directories": "0.0.1",
    "run-sequence": "^1.1.5",
    "webpack": "^1.12.14",
    "yargs": "^4.6.0"
  },
  "devDependencies": {
    "babel-eslint": "^6.0.3",
    "chai": "^3.5.0",
    "coveralls": "^2.11.11",
    "eslint": "^2.8.0",
    "gulp-bump": "^2.1.0",
    "gulp-eslint": "^2.0.0",
    "isparta-loader": "^2.0.0",
    "karma": "^0.13.21",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^0.2.3",
    "karma-coverage": "^0.5.5",
    "karma-firefox-launcher": "^0.1.7",
    "karma-ie-launcher": "^0.2.0",
    "karma-mocha": "^0.2.2",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sinon": "^1.0.4",
    "karma-sinon-chai": "^1.2.2",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.4.5",
    "phantomjs-prebuilt": "^2.1.7",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0"
  }
}
